{
    "manifest_version": 3,
    "name": "FB Group Phone Extractor",
    "version": "1.0.0",
    "description": "Extract phone numbers from Facebook Group posts",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["https://www.facebook.com/*"],
    "action": {
      "default_popup": "popup.html"
    }
  }
  